FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 6e-5;

boundaryField
{
    duct
    {
        type            fixedValue;
        value           uniform 0;
    }
    baffle
    {
	type		cyclic;
    }
    baffleother
    {
    	type		cyclic;
    }
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    tunnel {
        type            fixedValue;
        value           uniform 0;
    }
    wedge0{ type wedge; }
    wedge1{ type wedge; }
}


